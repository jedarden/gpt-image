{
  "name": "gpt-image-1-ui",
  "version": "1.0.0",
  "description": "GPT-Image-1 UI: Fullstack React/Express/TypeScript project",
  "main": "backend/server.ts",
  "scripts": {
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "jest --config frontend/jest.config.js",
    "test:backend": "jest --config backend/jest.config.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "tsc --project frontend/tsconfig.json",
    "build:backend": "tsc --project backend/tsconfig.json",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.18",
    "@types/express-rate-limit": "^5.1.3",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.17",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.3.4",
    "typescript": "^5.4.5"
  },
